<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GdpGraphViewController">
    <children>
        <Button fx:id="tableView" layoutX="512.0" layoutY="264.0" mnemonicParsing="false" onAction="#viewTable" text="View Table" />
      <BarChart fx:id="barChart" layoutX="14.0">
        <xAxis>
          <CategoryAxis fx:id="quarterAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="percentAxis" side="LEFT" />
        </yAxis>
      </BarChart>
    </children>
</AnchorPane>
